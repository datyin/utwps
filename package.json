{
  "name": "utwps",
  "version": "0.0.9",
  "description": "Just a 'tree-shakeable' js/ts library for getting and checking safe value of specific type",
  "type": "module",
  "scripts": {
    "test": "vitest --ui",
    "build": "tsc --noEmit --emitDeclarationOnly false && vite build && tsc",
    "dev": "vite build --mode development"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "typings": "./dist/typings/index.d.ts",
  "types": "./dist/typings/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/typings/index.d.ts",
      "import": "./dist/dist/index.js",
      "require": "./dist/dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "author": "Jove Jonovski <jovejonovski@gmail.com> (https://github.com/datyin)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/datyin/utwps"
  },
  "readme": "https://github.com/datyin/utwps/README.md",
  "bugs": {
    "url": "https://github.com/datyin/utwps/issues"
  },
  "devDependencies": {
    "@datyin/eslint-config-ts": "^0.0.7",
    "@types/node": "^18.11.9",
    "@vitest/ui": "^0.24.5",
    "eslint": "^8.27.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.2",
    "vitest": "^0.24.5"
  },
  "keywords": [
    "types",
    "get",
    "string",
    "number",
    "boolean",
    "is",
    "modules",
    "stdlib",
    "util",
    "wrapper"
  ]
}
