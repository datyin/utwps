{
  "name": "utwps",
  "version": "0.0.2",
  "description": "Just an tree-shakeable js/ts library for getting/checking safe value of specific type",
  "type": "module",
  "scripts": {
    "test": "vitest --ui",
    "build": "vite build",
    "dev": "vite build --mode development",
    "dts": "tsc --emitDeclarationOnly --declaration --declarationDir ./typings"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "typings": "./typings/index.d.ts",
  "types": "./typings/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/node/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "typings"
  ],
  "author": "Jove Jonovski <jovejonovski@gmail.com> (https://github.com/datyin)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/datyin/utwps"
  },
  "readme": "https://github.com/datyin/utwps/README.md",
  "bugs": {
    "url": "https://github.com/datyin/utwps/issues"
  },
  "devDependencies": {
    "@types/node": "^18.11.2",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@vitest/ui": "^0.24.3",
    "eslint": "^8.25.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.1.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vitest": "^0.24.3"
  },
  "keywords": [
    "types",
    "get",
    "string",
    "number",
    "boolean",
    "is",
    "modules",
    "stdlib",
    "util",
    "wrapper"
  ]
}
