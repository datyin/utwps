{
  "name": "utwps",
  "version": "0.0.13",
  "description": "Just a 'tree-shakeable' js/ts library for getting and checking safe value of specific type",
  "type": "module",
  "scripts": {
    "test": "vitest --ui",
    "build": "tsc --noEmit --emitDeclarationOnly false && vite build && tsc",
    "dev": "vite build --mode development",
    "lint": "eslint ./src"
  },
  "main": "./index.cjs",
  "module": "./index.js",
  "typings": "./typings/index.d.ts",
  "types": "./typings/index.d.ts",
  "exports": {
    ".": {
      "types": "./typings/index.d.ts",
      "import": "./index.js",
      "require": "./index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "author": "Jove Jonovski <jovejonovski@gmail.com> (https://github.com/datyin)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/datyin/utwps"
  },
  "readme": "https://github.com/datyin/utwps/README.md",
  "bugs": {
    "url": "https://github.com/datyin/utwps/issues"
  },
  "devDependencies": {
    "@datyin/eslint-config-ts": "^0.0.11",
    "@types/node": "^18.11.15",
    "@vitest/ui": "^0.25.8",
    "eslint": "^8.29.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.1",
    "vitest": "^0.25.8"
  },
  "keywords": [
    "types",
    "get",
    "string",
    "number",
    "boolean",
    "is",
    "modules",
    "stdlib",
    "util",
    "wrapper"
  ]
}
